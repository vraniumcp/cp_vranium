# USE cp_make FILENAME to make a file with init-template
mkdir "$1"
cd "$1"
if [[ "$2" == "u" ]]
  then
    cp "/Users/varunrajesh/Desktop/cp_vranium/init/tmpUSACO.cpp" "${1}.cpp"
elif [[ "$2" == "g" ]]
  then
    cp "/Users/varunrajesh/Desktop/cp_vranium/init/tmpGOOGLE.cpp" "${1}.cpp"
else
    cp "/Users/varunrajesh/Desktop/cp_vranium/init/tmp.cpp" "${1}.cpp"
fi